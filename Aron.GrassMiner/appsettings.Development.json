{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Debug"
    }
  },
  "app": {
    "ShowChrome": false,
    "GR_USER": "{GR_USER}",
    "GR_PASS": "{GR_PASS}",
    "AdminUserName": "{ADMIN_USER}",
    "AdminPassword": "{ADMIN_PASS}",
    "ProxyEnable": "{PROXY_ENABLE}", //"true" or "false"
    "ProxyHost": "{PROXY_HOST}", // "http(s)://proxy:port"
    "ProxyUser": "{PROXY_USER}",
    "ProxyPass": "{PROXY_PASS}",
    "LogEnable": false // "true" or "false""
  }
}
