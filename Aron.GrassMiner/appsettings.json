{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=db/Grass.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "app": {
    "ShowChrome": false,
    "UserName": "{GRASS_USER}",
    "Password": "{GRASS_PASS}",
    "AdminUserName": "{ADMIN_USER}",
    "AdminPassword": "{ADMIN_PASS}"
  }
}
